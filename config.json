{
  "baseURL": "https://victorianobennett.wedding",
  "languageCode": "en-us",
  "title": "Nick & Melissa's Wedding",
  "show_comments": false,
  "disableKinds": [
    "sitemap",
    "RSS"
  ],
  "markup": {
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
      "extensions": {
        "definitionList": true,
        "footnote": true,
        "linkify": true,
        "linkifyProtocol": "https",
        "strikethrough": true,
        "table": true,
        "taskList": true,
        "typographer": true
      },
      "parser": {
        "attribute": {
          "block": false,
          "title": true
        },
        "autoHeadingID": true,
        "autoHeadingIDType": "github"
      },
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  },
  "params": {
    "background_image_1": "https://res.cloudinary.com/dfwc6rovq/image/upload/v1719765475/B47Y5003_arevys.jpg",
    "background_image_2": "https://res.cloudinary.com/dfwc6rovq/image/upload/v1719765488/B47Y5057_wmbnqi.jpg",
    "background_image_3": "https://res.cloudinary.com/dfwc6rovq/image/upload/v1719766014/B47Y4875_y5d3yr.jpg"
  },
  "module": {
    "imports": [
      {
        "path": "github.com/future-wd/hugo-responsive-images",
        "path": "github.com/UtkarshVerma/hugo-modules/responsive-images"
      }
    ]
  }
}
